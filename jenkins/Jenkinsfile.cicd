node {
    checkout()
    deploy()
}

def checkout() {
    stage('Clone') {
        checkout scm
    }
}

def deploy() {
    stage('Deploy') {
        withKubeConfig([credentialsId: 'mykubeconfig']) {
            sh 'ls -la'
            sh 'pwd'
            sh '/var/jenkins_home/workspace/'kube test'/'kube command'/kubectl apply -f deployment.yaml'
        }
    }
}
